<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="MVCWebDemo.Models" assembly="MVCWebDemo" >
  <class name="Station" table="STATION">
    <id name="station_id" column="STATION_ID">
      <generator class="assigned"/>
    </id>
    <property name="capacity" column="CAPACITY"/>
    <property name="remain_capacity" column="REMAIN_CAPACITY"/>
    <property name="address" column="ADDRESS"/>
  </class>
</hibernate-mapping>